			function back(){
				if(q==0){
					q=5;
					document.getElementById("resim").src = arr[q-1].src;
					document.getElementById("first").src = arr[q-1].src;


					q--;
				}else{
					document.getElementById("resim").src = arr[q-1].src;
					document.getElementById("first").src = arr[q-1].src;
					
					q--;
				}
			}
			
			function next(){
				if(q==4){
					q=-1;
					document.getElementById("resim").src = arr[q+1].src;
					document.getElementById("first").src = arr[q+1].src;
				
					q++;
				}else{
					document.getElementById("resim").src=arr[q+1].src;
					document.getElementById("first").src = arr[q+1].src;
					
					q++;
					
			}








		}var v=1;

	
		function back2(){
				if(v==0){
					v=4;
					document.getElementById("second").src=arr[v].src;
				
					
				}
				else{
				v-=1;
				document.getElementById("second").src=arr[v].src;
									
			}


			}

			function next2(){
			
				if(v==4){
					document.getElementById("second").src=arr[0].src;
					v=0;
				}
				else{
					v+=1;
					document.getElementById("second").src=arr[v].src;				}
			}

			

		





