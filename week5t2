		function apa(){
			caas-=1;
			if(caas==-1){
				caas=4;
			}
			document.getElementById("PA").innerHTML = pass[caas];

		}
		function apa2(){
			caas+=1;
			if(caas==5){
				caas=0;
			}
			document.getElementById("PA").innerHTML = pass[caas];
		}
    

